{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/front-api/src",
  "monorepo": true,
  "root": "apps/front-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/front-api/tsconfig.app.json"
  },
  "projects": {
    "front-api": {
      "type": "application",
      "root": "apps/front-api",
      "entryFile": "main",
      "sourceRoot": "apps/front-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/front-api/tsconfig.app.json"
      }
    },
    "admin-api": {
      "type": "application",
      "root": "apps/admin-api",
      "entryFile": "main",
      "sourceRoot": "apps/admin-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-api/tsconfig.app.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "cli": {
      "type": "application",
      "root": "apps/cli",
      "entryFile": "main",
      "sourceRoot": "apps/cli/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cli/tsconfig.app.json"
      }
    },
    "typegoose": {
      "type": "library",
      "root": "libs/typegoose",
      "entryFile": "index",
      "sourceRoot": "libs/typegoose/src",
      "compilerOptions": {
        "tsConfigPath": "libs/typegoose/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "graphql": {
      "type": "library",
      "root": "libs/graphql",
      "entryFile": "index",
      "sourceRoot": "libs/graphql/src",
      "compilerOptions": {
        "tsConfigPath": "libs/graphql/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    }
  }
}